{{#view 'fileupload'}}
	<div class="cms-tramites-modal-dialog fileupload-dialog modal fade" id="fileupload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				
				<div class="panel panel-white panel-steps">
					<header class="panel-heading">Carga de formato digital</header>			
				</div>
				<div class="modal-body">
					<form id="fileUploadForm" {{action 'fileUpload' on="subimit"}} enctype="multipart/form-data">
						<div class="col-sm-10 col-md-push-2 nopadding">
							<div class="form-group">
								<label>Seleccionar documento</label><br>
								{{input type="file" class="form-control file-form" name="attachment" valueBinding="file"}}
							</div>
						</div>
					</form>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-action">Subir formato</button>
				</div>
			</div>
		</div>
	</div>
{{/view}}